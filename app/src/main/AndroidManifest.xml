<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookie.developdraw">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ADrawSimpleActivity"/>
        <activity android:name=".ADrawLineActivity"/>
        <activity android:name=".ADrawTextPathActivity"/>
        <activity android:name=".ADrawRegionActivity"/>
        <activity android:name=".ADrawRegionClipActivity"/>
        <activity android:name=".BDrawTranslateActivity"/>
        <activity android:name=".BClipRectActivity"/>
    </application>

</manifest>